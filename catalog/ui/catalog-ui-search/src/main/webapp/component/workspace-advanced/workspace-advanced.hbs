<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Name
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{name}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Created
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{createdDate}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Modified
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{lastModifiedDate}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        ID
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{id}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Source
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{source}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Shared With
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{#each sharedWith}}
            <span class="workspace-shared-group">
                {{this}}
            </span>
        {{/each}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Searches
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{numSearches}}
    </div>
</div>
<div class="workspaceBasic-row clearfix">
    <div class="workspaceBasic-property workspaceBasic-cell">
        Saved Items
    </div>
    <div class="workspaceBasic-value workspaceBasic-cell">
        {{numSavedItems}}
    </div>
</div>
