    <circle fill="{{this.fill}}" cx="50%" cy="50%" r="25%" stroke="{{this.outline}}"
            stroke-width="3%"></circle>
    <text fill="{{this.textFill}}" y="50%" x="50%" text-anchor="middle" dominant-baseline="central"
          font-family="'Helvetica Neue',Helvetica,Arial,sans-serif" font-size="30">
