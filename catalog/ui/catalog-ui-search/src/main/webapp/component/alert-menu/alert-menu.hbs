{{#if this.workspace}}
    <span class="menu-title workspace-title" data-workspaceid="{{this.workspace.id}}">
        {{this.workspace.title}}
    </span>
{{else}}
    <span class="menu-title">
        Unknown Workspace
    </span>
{{/if}}
<span class="menu-title title-seperator fa fa-caret-right">

</span>
{{#if this.query}}
    <span class="menu-title query-title" data-workspaceid="{{this.query.id}}">
        {{this.query.title}}
    </span>
{{else}}
    <span class="menu-title" >
        Unknown Query
    </span>
{{/if}}
<span class="menu-title title-seperator fa fa-caret-right">

</span>