<div class="workspaceHistory-header clearfix">
    <div class="workspaceHistory-row clearfix">
        <div class="workspaceHistory-cell workspaceHistory-version">
            Version
        </div>
        <div class="workspaceHistory-cell workspaceHistory-date">
            Date
        </div>
        <div class="workspaceHistory-cell workspaceHistory-user">
            Modified by
        </div>
    </div>
</div>
<div class="workspaceHistory-body is-list">
    {{#each history}}
        <div class="workspaceHistory-row clearfix is-button" data-id="{{id}}">
            <div class="workspaceHistory-cell workspaceHistory-version">
                {{version}}
            </div>
            <div class="workspaceHistory-cell workspaceHistory-date">
                {{date}}
            </div>
            <div class="workspaceHistory-cell workspaceHistory-user">
                {{user}}
            </div>
        </div>
    {{/each}}
</div>
<button class="is-primary">
    <span class="fa fa-undo"></span>
    Revert to Selected Version
</button>