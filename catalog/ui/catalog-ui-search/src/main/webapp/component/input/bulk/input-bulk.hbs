<div class="if-editing">
    <div class="enum-region">

    </div>
</div>
<div class="if-viewing">
    <label class="value-list-header">
        (Multiple Values)
    </label>
    <div class="value-list is-list">
        {{#each values}}
            <div class="list-value" data-ids="{{this.ids}}">
                <span class="cell-value">{{this.value}}</span>
                <span class="cell-hits">({{this.hits}})</span>
                <span class="cell-validation fa fa-exclamation-triangle is-hidden"></span>
            </div>
        {{/each}}
    </div>
</div>
<div class="if-homogeneous if-other">
    <div class="input-other">

    </div>
</div>